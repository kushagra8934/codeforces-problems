#include <iostream>
#include <bits/stdc++.h>
using namespace std;
void stringString(){
    string ans;
    int n=8;
    for(int i=0;i<n;i++){
        string a;
        cin>> a;
        ans+=a;
    }
    string temp;
    for(int i=0;i<ans.size();i++){
        if(ans[i] != '.'){
            temp+=ans[i];
        }
    }
    cout<< temp << endl;
    
}

int main()
{
    int t;
    cin>>t;
    while(t--){
        stringString();
    }

    return 0;
}
