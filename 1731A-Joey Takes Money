#include <iostream>
using namespace std;
void solve(){
    int n;
    cin>> n;
    long long temp=1;
    for(int i=0;i<n;i++){
        long long x;
        cin>> x;
        temp *= x;
    }
    temp += (n-1);
    cout<< temp*2022 << endl;
}
int main()
{
    int t;
    cin>> t;
    while(t--){
        solve();
    }
    return 0;
}
